{
    "bash": {
        "container": "debian:latest",
        "mimeTypes": [
            "application/x-sh",
            "application/x-bash",
            "text/x-sh",
            "text/x-shellscript"
        ]
    },
    "php": {
        "container": "php:latest",
        "mimeTypes": [
            "application/x-httpd-php",
            "application/x-php",
            "text/x-php"
        ]
    },
    "python": {
        "container": "python:latest",
        "mimeTypes": [
            "application/x-python-code",
            "application/x-python",
            "text/x-python"
        ]
    },
    "ruby": {
        "container": "ruby:latest",
        "mimeTypes": [
            "application/x-ruby",
            "text/x-ruby"
        ]
    },
    "javascript": {
        "container": "node:latest",
        "mimeTypes": [
            "application/javascript",
            "text/javascript",
            "application/x-javascript"
        ]
    },
    "go": {
        "container": "golang:latest",
        "mimeTypes": [
            "application/x-go",
            "text/x-go",
            "text/x-go-source"
        ]
    },
    "c": {
        "container": "gcc:latest",
        "mimeTypes": [
            "text/x-c",
            "text/x-c-header",
            "application/x-c",
            "application/x-c-header"
