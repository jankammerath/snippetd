    "java": {
        "container": "openjdk:latest",
        "mimeTypes": [
            "text/x-java-source",
            "text/x-java",
            "application/x-java-source",
            "application/x-java",
            "application/java"
        ]
    },
    "rust": {
        "container": "rust:latest",
        "mimeTypes": [
            "text/x-rust"
        ]
